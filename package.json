		"swiper": "^12.0.3"
	},
	"devDependencies": {
		"@babel/core": "^7.12.0",
		"@parcel/transformer-js": "2.13.3",
		"@parcel/transformer-sass": "^2.13.3",
		"buffer": "^5.5.0||^6.0.0",
		"process": "^0.11.10",
		"sass": "^1.84.0",
